{
  "name": "@spruceid/ssx-react",
  "version": "1.1.1",
  "description": "SSX React Hooks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.js",
      "module": "./dist/esm/index.js",
      "default": "./dist/index.js"
    },
    "./siwe": {
      "types": "./dist/siwe.d.js",
      "module": "./dist/esm/siwe.js",
      "default": "./dist/siwe.js"
    },
    "./next-auth/backend": {
      "types": "./dist/next-auth/backend.d.js",
      "module": "./dist/esm/next-auth/backend.js",
      "default": "./dist/next-auth/backend.js"
    },
    "./next-auth/frontend": {
      "types": "./dist/next-auth/frontend.d.js",
      "module": "./dist/esm/next-auth/frontend.d.js",
      "default": "./dist/next-auth/frontend.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "siwe": [
        "dist/siwe.d.ts"
      ],
      "next-auth/backend": [
        "dist/next-auth/backend.d.ts"
      ],
      "next-auth/frontend": [
        "dist/next-auth/frontend.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:spruceid/ssx.git"
  },
  "type": "module",
  "author": "Spruce Systems, Inc.",
  "license": "Apache-2.0 OR MIT",
  "private": false,
  "scripts": {
    "build": "tsup",
    "build2": "yarn clean && rollup -c",
    "build3": "unbuild",
    "clean": "rm -rf dist",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@spruceid/ssx": "1.1.1",
    "@spruceid/ssx-server": "1.1.1",
    "next": "^12.1.6",
    "next-auth": "^4.10.3",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "wagmi": "0.10.10",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "typescript": "^4.8.4",
    "tsup": "^6.5.0",
    "ethers": "^5.7.2",
    "unbuild": "1.1.1",
    "rollup": "^3.10.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@rollup/plugin-commonjs": "24.0.0"
  },
  "files": [
    "tsconfig.json",
    "dist",
    "next"
  ]
}
